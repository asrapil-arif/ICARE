	function format_currencyproc(Field_ID, temp_field) {

		if ($("#" + Field_ID).val() != "") {

			var Nominal = $("#" + Field_ID).val();
			Nominal = Nominal.replace(/,/g, "");
			$("#" + temp_field).val(Nominal);
			var fix_num = numberWithCommas(Nominal);

			$("#" + Field_ID).val(fix_num);

		}
		else {

			$("#" + temp_field).val("");

		}
	}

